{
  "exclude": "node_modules/**",
  "plugins": [
    "@babel/transform-flow-strip-types"
  ],
  "presets": [
    [
      "@babel/env",
      {
        "targets": {
          "browsers": [
            "last 5 chrome versions, last 5 firefox versions"
          ]
        },
        "useBuiltIns": false,
        "modules": false
      }
    ]
  ]
}
