{
  "author": {
    "email": "gajus@anuary.com",
    "name": "Gajus Kuizinas",
    "url": "http://gajus.com"
  },
  "ava": {
    "require": [
      "@babel/register",
      "esm"
    ],
    "babel": {
      "testOptions": {
        "plugins": [
          "@babel/transform-flow-strip-types"
        ]
      }
    }
  },
  "dependencies": {
    "debug": "^2.6.6",
    "load-script": "^1.0.0",
    "sister": "^3.0.0"
  },
  "description": "YouTube IFrame Player API abstraction.",
  "devDependencies": {
    "@ava/babel": "^1.0.1",
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.11.0",
    "@babel/plugin-transform-flow-strip-types": "^7.10.4",
    "@babel/polyfill": "^7.10.4",
    "@babel/preset-env": "^7.11.0",
    "@babel/register": "^7.10.5",
    "@rollup/plugin-babel": "^5.1.0",
    "@rollup/plugin-commonjs": "^14.0.0",
    "@rollup/plugin-node-resolve": "^8.4.0",
    "ava": "^3.11.1",
    "babel-eslint": "^10.1.0",
    "babelrc-rollup": "^3.0.0",
    "chai": "^3.5.0",
    "eslint": "^3.19.0",
    "eslint-config-canonical": "^8.2.1",
    "eslint-plugin-flowtype": "^5.2.0",
    "eslint-scope": "^5.1.0",
    "esm": "^3.2.25",
    "flow-bin": "^0.45.0",
    "flow-copy-source": "^1.1.0",
    "husky": "^0.13.3",
    "npm-watch": "^0.1.9",
    "rollup": "^2.23.0",
    "rollup-plugin-babel": "^4.4.0",
    "semantic-release": "^6.3.2"
  },
  "keywords": [
    "youtube",
    "iframe",
    "player",
    "api",
    "promise",
    "async",
    "video"
  ],
  "license": "BSD-3-Clause",
  "main": "./dist/index.js",
  "name": "youtube-player",
  "repository": {
    "type": "git",
    "url": "https://github.com/gajus/youtube-player"
  },
  "watch": {
    "test": "{src,test}/*.js"
  },
  "scripts": {
    "babel": "NODE_ENV=production babel ./src --out-dir ./dist --copy-files && flow-copy-source src dist",
    "build": "rollup -c",
    "lint": "eslint ./src ./test",
    "lint:fix": "eslint --fix ./src ./test",
    "watch": "npm-watch",
    "precommit": "npm run lint && npm run test",
    "test": "NODE_ENV=development ava --verbose"
  },
  "version": "5.0.1"
}
